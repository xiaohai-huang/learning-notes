    "@docusaurus/theme-live-codeblock": "3.0.0-beta.0",
    "@mdx-js/react": "^2.3.0",
    "aegis-web-sdk": "^1.38.3",
    "clsx": "^1.2.1",
    "docusaurus-plugin-sass": "^0.2.2",
    "object-inspect": "^1.12.0",
    "prism-react-renderer": "^1.3.5",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
