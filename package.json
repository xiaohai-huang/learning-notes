    "upgrade-docusaurus": "yarn add --exact @docusaurus/core@canary @docusaurus/plugin-content-docs @docusaurus/preset-classic@canary @docusaurus/theme-live-codeblock @docusaurus/module-type-aliases"
  },
  "dependencies": {
